{
  "name": "cohls-portfolio-frontend",
  "version": "1.0.0",
  "private": true,
  "dependencies": {
    "@types/node": "^16.18.12",
    "@types/react": "^18.0.28",
    "@types/react-dom": "^18.0.11",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.8.1",
    "react-scripts": "5.0.1"
  },
  "devDependencies": {
    "tailwindcss": "^3.2.7"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "aws:s3:sync": "aws s3 sync --profile twl --delete build/ s3://cohls-portfolio-bucket",
    "aws:cloudfront:invalidate": "WS_MAX_ATTEMPTS=10 aws cloudfront create-invalidation --profile twl --distribution-id E31JD5DY8LUVRW --paths \"/*\" > /dev/null",
    "deploy": "npm run build && npm run aws:s3:sync && npm run aws:cloudfront:invalidate",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
